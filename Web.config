<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="rule 1C" stopProcessing="true">
                    <match url=".*" />
                    <action type="Rewrite" url="/index.php?url={R:0}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
